  <Location "/Shibboleth.sso">
    SetHandler shib
  </Location>

  <Location />
    AuthType shibboleth
    Require shibboleth
  </Location>

  RewriteEngine On
  RewriteCond %{QUERY_STRING} !after_redirect
  RewriteRule ^/registry/auth/logout.* https://%%SERVER_NAME%%/Shibboleth.sso/Logout?return=https://%%SERVER_NAME%%/registry/auth/logout/?after_redirect [L,R]
